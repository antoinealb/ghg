#!/usr/bin/env python3

from github_go import *

def main():
    args = parse_arguments()

    if args.repository:
        url = 'https://github.com/{}'.format(args.repository)
        open_url(url)
    else:
        remote = get_remote()
        if remote is None:
            print('No remote')
        else:
            open_url(remote)

if __name__ == '__main__':
    main()
